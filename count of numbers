    // count of words
#include <stdio.h>
#include<string.h>
int main() 
{
    char a[20];
    printf("please enter the sentence: ");
    fgets(a,sizeof(a),stdin);
    int len=strlen(a),count=0;
    for(int i=0;i<len;i++)
    {
      if(a[i]==' '&&a[i+1]!=' ')
      {
          count=count+1;
      }
    }
    printf("No.of words:%d",count+1);
    
}
