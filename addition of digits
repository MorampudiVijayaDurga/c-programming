//list of menu
#include <stdio.h>
#include <conio.h>
int digits(int);
int res=0;
int digits(int n)
{


   res=res+(n%10);
   n=n/10;
   return res;
}

void main()
{
    int a[10]={12,22,33,44,55,66,77,88,99,100},sum=0;

    for(int i=0;i<10;i++)
    {
        sum=sum+digits(a[i]);
    }
    printf("sum of digits:%d",sum);

}
