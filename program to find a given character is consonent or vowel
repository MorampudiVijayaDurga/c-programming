#include <stdio.h>
int main()
{
    char ch;
    printf("ente any character:");
    scanf("%c",&ch);
    switch(ch)
    {

    case 'a':
    case 'A':
    case 'e':
    case 'E':
    case 'i':
    case 'I':
    case 'o':
    case 'O':
    case 'u':
    case 'U':
                printf("Given character is vowel");
                break;
    default:
                printf("Given character is consonent");

    }
}
