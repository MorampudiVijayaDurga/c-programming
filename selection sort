//selction sort
#include<stdio.h>
int main()
{
    int size;
    printf("Please enter the size of an array:");
    scanf("%d",&size);
    int arr[size],copy[size];
    printf("please enter the elements into array:");
    for(int i=0;i<size;i++)
    {
        scanf("%d",&arr[i]);
    }
    int temp,min;
    for(int i=0;i<size-1;i++)
    {
        min=i;
        for(int j=i+1;j<size;j++)
        {
            if(arr[j]<arr[min])
            {
                min=j;
            }
        }
        if(min!=i)
        {
            temp=arr[i];
            arr[i]=arr[min];
            arr[min]=temp;
        }
    }
    for(int i=0;i<size;i++)
    {
        printf("%d ",arr[i]);
    }
}
