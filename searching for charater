/searching character in string
#include <stdio.h>
#include <string.h>
void main()
{
 char a[20],c;
 printf("enter the first string:");
 scanf("%s",a);
 printf("enter the character you want to search:\n");
 scanf(" %c",&c);
 int len=strlen(a),found=0;
 for(int i=0;i<len;i++)
 {
     if(a[i]==c)
     {
      found=1;
      break;
     }

 }

if(found==1)
{

    printf("character found");
}
else{
    printf("character not found");
}



}
