//right shift of array
#include <stdio.h>
int main()
{
    int size,last;
    printf("Please enter the size of an array:");
    scanf("%d",&size);
    int arr[size],freq[size];
    printf("please enter the elements into array:");
    for(int i=0;i<size;i++)
    {
        scanf("%d",&arr[i]);
    }
    last=arr[size-1];
    for(int i=size-1;i>0;i--)
    {
        arr[i]=arr[i-1];
    }
    arr[0]=last;
    printf("Array after right shift:");
    for(int i=0;i<size;i++)
    {
        printf("%d",arr[i]);
    }
}
