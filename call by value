#include <stdio.h>
void swap(int a, int b);
int main()
{
    int a,b;
    printf("enter any two values:");
    scanf("%d%d",&a,&b);
    printf("\nbefore swapping the values are:%d %d",a,b);
    swap(a,b);
    printf("\nAfter swapping the values are:%d %d",a,b);
    return 0;
}
void swap(int p,int q)
{
    int temp;
    temp=p;
    p=q;
    q=temp;
}
