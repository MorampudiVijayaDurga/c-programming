// count of words
#include <stdio.h>
#include<string.h>
void main() 
{
    char a[20];
    printf("please enter the word");
    scanf("%s",a);
    int len =strlen(a);
    int i,j;
    for(i=0,j=len-1;i<=j;i++,j--)
    {
      if(a[i]!=a[j])
      {
        printf("Not palindrome");
        return;
      }
    }
    printf("palindrome");
}
