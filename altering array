// altering array
#include <stdio.h>
int i,j,arr[3][3]={{1,2,3},{4,5,6},{7,8,9}};
int main() 
{
 arr[0][0]=6;
 arr[0][1]=10;
 arr[0][2]=3;
 for(i=0;i<3;i++)
 {
     arr[i][1]=20;
 }
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            printf("%d\t",arr[i][j]);
        }
        printf("\n");
    }
    
}
