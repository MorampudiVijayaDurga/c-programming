//count of vowels
#include <stdio.h>
#include <string.h>
int main()
{
 char a[20];
 printf("enter the  string:");
 scanf("%s",a);
 int len=strlen(a);
 int i,count=0;
 for(i=0;i<len;i++)
 {
  if(a[i]=='a'||a[i]=='e'||a[i]=='i'||a[i]=='o'||a[i]=='u')
  {

    count=count+1;
  }
 }
 printf("%d",count);
}
