//frequency of each element in an array
#include<stdio.h>
int main()
{
    int size,count=0;
    printf("Please enter the size of an array:");
    scanf("%d",&size);
    int arr[size],freq[size];
    printf("please enter the elements into array:");
    for(int i=0;i<size;i++)
    {
        scanf("%d",&arr[i]);
        freq[i]=1;
    }

    for(int i=0;i<size;i++)
    {
      if(freq[i]==0)
        {
            continue;
        }
        for(int j=i+1;j<size;j++)
        {
            if(freq[j]==0)
            {
                continue;
            }
            if(arr[j]==arr[i])
            {
              freq[i]++;
              freq[j]=0;
            }
        }
    }
for(int i=0;i<size;i++)
{
    if(freq[i]!=0)
    printf("Frequency of %d is %d\n",arr[i],freq[i]);
}
}


