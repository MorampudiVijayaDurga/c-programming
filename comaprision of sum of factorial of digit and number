#include <stdio.h>
int main()
{
    int num,rem,sum=0;
    scanf("%d",&num);
    int org_num=num;
    while(num>0)
    {
      rem=num%10;
      int fact=1;
    for(int i=1;i<=rem;i++)
    {
      fact=fact*i;

    }

    sum=sum+fact;
    num=num/10;
    }
    if(sum==org_num)
    {

     printf("given number is a strong number");

    }
    else
    {
      printf("Not a strong number");

    }
}
