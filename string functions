//string built in fyunctions
#include <stdio.h>
#include <string.h>
int main()
{
 char a[2],b[20];
 printf("enter the first string:");
 gets(a);
 printf("enter the second string: ");
 gets(b);
 printf("comparision of two strings: %d\n",strcmp(a,b));
 printf("length of string:%d\n",strlen(a));
 printf("concatination of two strings :%s\n",strcat(a,b));
 printf("copy of strings :%s\n",strcpy(a,b));

}
